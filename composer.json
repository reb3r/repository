{
  "name": "reb3r/repositories",
  "description": "Laravel Repositories",
  "license": "MIT",
  "homepage": "https://bosnadev.com",
  "keywords": ["laravel", "repository", "repositories", "eloquent", "database"],
  "authors": [
    {
      "name": "Mirza Pasic",
      "email": "mirza@bosnadev.com"
    },
    {
      "name": "Maarten Paauw",
      "email": "maartenpaauw@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "^7.0",
    "illuminate/database": "^7.0",
    "illuminate/filesystem": "^7.0",
    "illuminate/console": "^7.0"
  },
  "require-dev": {
    "mockery/mockery": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "Bosnadev\\": "src/Bosnadev/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bosnadev\\Repositories\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
